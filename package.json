{
    "version": "0.1.6",
    "license": "MIT",
    "name": "ssd1351",
    "author": {
        "name": "Florent Mazzone",
        "email": "ftmazzone@yahoo.fr"
    },
    "contributors": [],
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/ftmazzone/ssd1351.git"
    },
    "dependencies": {
        "freetype2": "^0.3.1",
        "oled-font-5x7": "^1.0.3",
        "onoff": "^4.1.0",
        "spi-device": "^2.0.6"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^6.0.2",
        "nyc": "^13.3.0",
        "rewire": "^4.0.0"
    },
    "scripts": {
        "coverage": "node ./node_modules/nyc/bin/nyc.js ./node_modules/mocha/bin/_mocha '**/unitTests/*.js'",
        "test": "node ./node_modules/mocha/bin/_mocha '**/unitTests/*.js'"
    },
    "engines": {
        "node": ">=10.15.3"
    },
    "files": [
        "fonts/",
        "lib/"
    ],
    "main": "./lib",
    "keywords": [
        "oled",
        "spi",
        "raspberry-pi"
    ],
    "nyc": {
        "exclude": [
            "node_modules/**",
            "tests/"
        ],
        "check-coverage": true,
        "all": true,
        "report-dir": ".nyc_output/reports",
        "temp-directory": ".nyc_output/",
        "reporter": [
            "lcov",
            "text-summary",
            "html"
        ],
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
